{% form_theme form 'form.html.twig' %}

<div class="dialog">
    {{ form_start(form) }}
    <fieldset>
        {{ form_row(form.name) }}
        {{ form_row(form.prefix) }}
        {{ form_row(form.criticalAge) }}
        {{ form_row(form.frozenTime) }}
        {{ form_row(form.description) }}
    </fieldset>
    {{ form_end(form) }}
</div>
